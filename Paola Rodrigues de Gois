while True:
  print("****************************************")
  print("* calculadora da segunda lei de Newton *")
  print("****************************************\n")
  massa = int(input("qual é a massa do corpo? "))
  aceleracao = int(input("\nqual é a aceleração do corpo "))
  print("\nA força é equivalente a", massa * aceleracao,"Newtons\n\n")
